<Catalog>
    <!-- Regenerative Bio-Steel -->
    <CActorModel id="Kit@RegenerativeBioSteel" parent="ModelAnimationStyleContinuousUNIT">
        <EditorCategories value=""/>
        <On Terms="UnitRegen.*.LifeOn; ValidateUnit Kit@RegenerativeBioSteelOnUnit" Send="Create"/>
        <On Terms="UnitRegen.*.LifeOff" Send="AnimBracketStop BSD"/>
        <On Terms="ActorCreation" Send="Create Kit@RegenerativeBioSteelImpactSound"/>
        <On Terms="ActorCreation" Send="Create Kit@RegenerativeBioSteelGlaze"/>
        <On Terms="ActorDestruction" Target="Kit@RegenerativeBioSteelGlaze" Send="AnimBracketStop BSD"/>
        <HostSiteOps Ops="SOpAttachCenter" HoldRotation="1"/>
    </CActorModel>
    <CActorModelMaterial id="Kit@RegenerativeBioSteelGlaze" parent="GlazeParent">
        <On Terms="ActorCreation" Send="TimerSet 1.000000 DelayDestroy"/>
        <On Terms="TimerExpired; TimerName DelayDestroy" Send="AnimBracketStop BSD"/>
        <On Terms="ActorCreation" Send="ModelMaterialApplyTo ::Main"/>
        <ModelFlags index="AutomateGlobalLoops" value="1"/>
    </CActorModelMaterial>
    <CActorSound id="Kit@RegenerativeBioSteelImpactSound" parent="SoundOneShot">
        <Sound value="Kit@RegenerativeBioSteelImpact"/>
    </CActorSound>
