    <!-- Spectre Ultrasonic Pulse Impact Animation Fix -->
    <CActorModel id="UltrasonicPulseAttackImpactEffect">
        <Inherits index="Visibility" value="1"/>
        <On Terms="Effect.UltrasonicPulseSearchArea.Start" Send="Create"/>
    </CActorModel>
