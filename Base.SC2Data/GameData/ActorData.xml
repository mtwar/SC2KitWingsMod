        <On Terms="Abil.ChannelSnipe.SourceCastStop" Send="AnimBaselineStart"/>
        <On Terms="Abil.ChannelSnipe.SourceChannelStart" Send="AnimPlay SpellB Spell,B PlayForever"/>
        <On Terms="Abil.ChannelSnipe.SourceChannelStop" Send="AnimClear SpellB"/>
        <On Terms="Effect.ChannelSnipeDamage.Start; At Caster" Send="AnimPlay SpellC Spell,C"/>
        <On Terms="Effect.ChannelSnipeInitialSet.Start; At Caster" Send="Create ChannelSnipeTrackingBeam"/>
    </CActorUnit>
    <CActorModel id="ChannelSnipeLaunchSquib" parent="ModelAnimationStyleOneShot">
        <Flags index="SuppressCreationErrors" value="1"/>
        <Host Subject="_Unit"/>
        <HostSiteOps Ops="SOpAttachWeapon"/>
        <On Terms="Effect.ChannelSnipeDamage.Start; At Caster" Send="Create"/>
        <Model value="GhostSnipeAttackLaunch"/>
    </CActorModel>
    <CActorRange id="ChannelSnipeRange" parent="RangeAbil" abil="ChannelSnipe">
        <Range value="10.000000"/>
        <Icon value="Assets\Textures\RadarIcon2.dds"/>
        <IconArcLength value="3.250000"/>
    </CActorRange>
    <CActorAction id="ChannelSnipe" parent="GenericAttack" effectAttack="ChannelSnipeDamage">
        <LaunchSiteOps Ops="SOpAttachHarnessStationary"/>
        <LaunchAssets Sound="Ghost_SnipeLaunch"/>
        <ImpactMap index="None" Sound="Ghost_SnipeImpact"/>
    </CActorAction>
    <CActorBeamSimple id="ChannelSnipeBeam" parent="BeamSimpleAnimationStyleOneShot">
        <HostLaunch Subject="_Unit"/>
        <HostImpactSiteOps Ops="SOpTargetUnit SOpAttachHead" HoldPosition="1" HoldRotation="1"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="ChannelSnipeTrackingBeam" parent="BeamSimpleAnimationStyleContinuous">
        <HostLaunch Subject="_Unit" Actor="Find"/>
        <HostLaunchSiteOps Ops="SOpAttachWeapon"/>
        <HostImpactSiteOps Ops="SOpTargetUnit SOpAttachHead"/>
        <On Terms="Effect.ChannelSnipeCombatRB.Start" Send="Destroy"/>
    </CActorBeamSimple>
    <!-- Faster Nukes for Player -->
    <CActorModel id="GhostNukeIndicator">
        <On Terms="Effect.Kit@NukeFastPersistent.Start" Send="Create"/>
        <On Terms="Effect.Kit@NukeFastPersistent.Start" Send="AnimPlay Fast Stand 0 0.000000 0.000000 10.000000 AsDuration"/>
        <On Terms="Effect.Kit@NukeFastPersistent.Stop" Send="Destroy"/>
    </CActorModel>
    <CActorSquib id="GhostNukeLaunch">
        <On Terms="Effect.Kit@NukeFastPersistent.Start" Send="Create"/>
    </CActorSquib>
    <CActorSound id="GhostNukeSeek">
        <On Terms="Effect.Kit@NukeFastPersistent.Initial" Send="Create"/>
        <On Terms="Effect.Kit@NukeFastPersistent.Stop; At Effect" Send="Destroy"/>
    </CActorSound>
    <CActorModel id="GhostNukeTarget">
        <On Terms="Effect.Kit@NukeFastPersistent.Start" Send="Create"/>
        <On Terms="Effect.Kit@NukeFastPersistent.Stop" Send="AnimBracketStop BSD"/>
    </CActorModel>
    <!-- Spectre fix Nukes and Ultrasonic -->
    <CActorUnit id="SpecterUnit">
        <On Terms="Abil.TacNukeStrike.SourceCastStart" Send="AnimPlay SpellA Spell,A"/>
        <On Terms="Abil.TacNukeStrike.SourceCastStop" Send="AnimClear SpellA"/>
        <On Terms="Abil.TacNukeStrike.SourceChannelStart" Send="AnimPlay SpellB Spell,B PlayForever"/>
        <On Terms="Abil.TacNukeStrike.SourceChannelStop" Send="AnimClear SpellB"/>
        <On Terms="Abil.TacNukeStrike.SourceFinishStart" Send="AnimPlay SpellC Spell,C"/>
        <On Terms="Abil.TacNukeStrike.SourceFinishStop" Send="AnimClear SpellC"/>
        <On Terms="Abil.UltrasonicPulse.SourceCastStart" Send="AnimPlay Spell Attack 0 0.000000"/>
        <On Terms="Abil.UltrasonicPulse.SourceFinishStop" Send="AnimClear Spell"/>
        <Remove Terms="Abil.UltrasonicPulse.SourceCastStart" Send="AnimPlay Attack Attack 0 0.000000"/>
        <Remove Terms="Abil.UltrasonicPulse.SourceCastStart" Send="AnimSetTimeScale Attack 2.500000"/>
