            </Frame>

            <Animation name="Show">
                <Event event="Birth" action="Reset,Play" frame="$this"/>

                <Controller name="Fade_0" type="Fade" end="Pause" frame="$this">
                    <Key type="CurveReal" time="0" inout="Linear,Slow" value="0"/>
                    <Key type="CurveReal" time="1.3" inout="Slow,Linear" value="255"/>
                </Controller>
            </Animation>

            <StateGroup name="RotationGroup">
                <DefaultState val="Default"/>
                <State name="45">
                    <When type="AnimationState" Rotation="45"/>
                    <Action type="SetProperty" Rotation="45"/>
                </State>
                <State name="Default">
                    <When type="AnimationState" Rotation="Default"/>
                    <Action type="SetProperty" Rotation="0"/>
                </State>
            </StateGroup>
        </Frame>

