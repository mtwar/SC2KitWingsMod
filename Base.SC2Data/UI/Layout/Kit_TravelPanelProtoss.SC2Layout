                <Frame type="Label" name="Heading">
                    <Text val="@UI/StoryMissionLaunch/BonusRewards"/>
                </Frame>
            </Frame>
        </Frame>	
        
        <Frame type="DifficultyPulldown" name="DifficultyPulldown" template="VoidShared_GenericTemplates/DifficultyPulldownTemplate">
