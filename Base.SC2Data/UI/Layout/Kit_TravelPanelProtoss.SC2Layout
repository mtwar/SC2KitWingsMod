            </Frame>
            
            <Frame type="Button" name="StopButton" template="VoidShared_GenericTemplates/StopButtonTemplate">
                <Anchor relative="$parent"/>
                <Shortcut val="W"/>
                <Visible val="false"/>
            </Frame>
        </Frame>

        
