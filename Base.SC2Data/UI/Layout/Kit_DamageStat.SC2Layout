                <Text val="99,999"/>
            </Frame>

            <Frame type="Image" name="Icon3" template="Kit_DamageStat/IconTemplate">
                <Anchor side="Top" relative="$parent/Icon2" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent/Icon2" pos="Min" offset="0"/>
